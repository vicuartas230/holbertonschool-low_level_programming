CC   = gcc
SRC  = main.c holberton.c
OBJ  = main.o holberton.o
NAME = holberton
RM   = rm -f
all : $(OBJ)
	$(CC) $(OBJ) -o $(NAME)
main.o : main.c
	$(CC) -c -o main.o main.c
holberton.o : holberton.c
	$(CC) -c -o holberton.o holberton.c
clean:
	$(RM) *~ $(NAME)
oclean:
	$(RM) $(OBJ)
fclean:
	$(RM) *~ $(NAME)
	$(RM) $(OBJ)
re:
	$(RM) $(OBJ)
	$(MAKE) all -f 3-Makefile
